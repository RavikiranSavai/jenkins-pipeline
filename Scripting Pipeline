 node {
 	stage('Build') {
 		echo "Build"
 	}
 	stage('Test') {
 		echo "Test"
 	}
	 stage('Deploy') {
 		echo "Test"
 	}stage('post') {
 		echo "Test"
 	}
 }
node {
	stage('build'){
		echo "Build"
	}
	stage('test'){
		echo "Deploy"
	}
	stage('Deploy'){
		echo "Deploy"
	}stage('Post'){
		echo "Post"
	}
}
